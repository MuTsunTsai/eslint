{
	"name": "@mutsuntsai/eslint",
	"version": "1.3.1",
	"description": "Customized ESLint config.",
	"keywords": [
		"eslint"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/MuTsunTsai/eslint"
	},
	"license": "MIT",
	"author": "Mu-Tsun Tsai <don.m.t.tsai@gmail.com> (https://github.com/MuTsunTsai)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rslib build",
		"dev": "rslib build --watch",
		"lint": "eslint .",
		"lint:inspect": "eslint-config-inspector",
		"test": "mocha"
	},
	"dependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/config-helpers": "^0.3.0",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.29.0",
		"@isaacs/brace-expansion": "^5.0.0",
		"@stylistic/eslint-plugin": "^5.0.0",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-html": "^8.1.3",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-mocha": "^11.1.0",
		"eslint-plugin-package-json": "^0.42.0",
		"eslint-plugin-playwright": "^2.2.0",
		"eslint-plugin-vue": "^10.2.0",
		"globals": "^16.2.0",
		"sort-package-json": "^3.3.1",
		"typescript-eslint": "^8.35.0"
	},
	"devDependencies": {
		"@eslint/config-inspector": "^1.1.0",
		"@hyperse/ts-node": "^1.0.3",
		"@microsoft/api-extractor": "^7.52.8",
		"@rslib/core": "^0.10.3",
		"@types/chai": "^5.2.2",
		"@types/estree": "^1.0.8",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.0.4",
		"chai": "^5.2.0",
		"eslint": "^9.29.0",
		"mocha": "^11.7.1"
	},
	"peerDependencies": {
		"eslint": "^9.5.0"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowAny": [
				"eslint"
			]
		},
		"onlyBuiltDependencies": [
			"@swc/core",
			"core-js",
			"esbuild",
			"unrs-resolver"
		]
	}
}

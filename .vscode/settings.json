{
	"cSpell.words": [
		"rslib"
	],
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"eslint.useFlatConfig": true,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.expand": false,
	"explorer.fileNesting.patterns": {
		"*.css": "$(capture).css.map",
		"*.js": "$(capture).prod.js, $(capture).min.js, $(capture).js.map",
		"*.woff2": "$(capture).eot, $(capture).svg, $(capture).ttf, $(capture).woff",
		".gitignore": ".gitattributes",
		".mocharc.json": ".nycrc",
		"README.md": "LICENSE.md, CONTRIBUTING.md",
		"eslint.config.js": ".editorconfig",
		"package.json": "pnpm-*.yaml, .npmrc, .browserslistrc, knip.config.ts"
	},
	"files.exclude": {
		".rslib/": true
	},
	"mochaExplorer.multiFileSuites": true,
	"mochaExplorer.nodeArgv": [
		"--enable-source-maps"
	]
}